{
    "name": "pluk77/doctrine-extensions",
    "type": "library",
    "description": "Doctrine2 behavioural extensions",
    "keywords": [
        "behaviors",
        "doctrine2",
        "extensions",
        "gedmo",
        "sluggable",
        "loggable",
        "translatable",
        "tree",
        "nestedset",
        "sortable",
        "timestampable",
        "blameable",
        "uploadable",
		"auditable",
		"pluk77"
    ],
    "homepage": "http://gediminasm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Gediminas Morkevicius",
            "email": "gediminas.morkevicius@gmail.com"
        },
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com"
        },
        {
            "name": "David Buchmann",
            "email": "david@liip.ch"
        },
		{
			"name": "Marcel Berteler"
			"email": "marcel.berteler@capetown.gov.za"
        }
    ],
    "require": {
        "php": ">=5.4",
        "behat/transliterator": "~1.2",
        "doctrine/common": "~2.4"
    },
    "replace": {
        "ext-mongo": "1.6.12"
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "~1.0.4",
        "doctrine/mongodb-odm": ">=1.0.2",
        "doctrine/orm": ">=2.5.0",
        "doctrine/common": ">=2.5.0",
        "symfony/yaml": "~2.6|~3.0|~4.0",
        "phpunit/phpunit": "^4.8|^5.7|^6.5"
    },
    "suggest": {
        "doctrine/mongodb-odm": "to use the extensions with the MongoDB ODM",
        "doctrine/orm": "to use the extensions with the ORM"
    },
    "autoload": {
        "psr-4": { "Gedmo\\": "lib/Gedmo" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
